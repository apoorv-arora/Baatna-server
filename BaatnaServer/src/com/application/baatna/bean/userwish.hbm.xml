<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.application.baatna.bean.UserWish" table="USERWISH">
		<id column="SERIAL" name="serial" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property name="wishId" column="WISHID" type="java.lang.Integer" />
		<property name="userId" column="USERID" type="java.lang.Integer" />
		<property name="wishStatus" column="WISH_STATUS" type="java.lang.Integer" />
		<property name="userTwoId" column="USER_TWO_ID" type="java.lang.Integer" />
		<property name="negotiationStatus" column="NEGOTIATION_STATUS" type="java.lang.Boolean" />
		<property name="negotiationAmount" column="NEGOTIATION_AMOUNT" type="java.lang.Integer" />
		<property name="u1ratedu2" type="java.lang.Double">
 		<column name="U1RATEDU2" not-null="true" default="0" />
 		</property>
 	     <property name="u2ratedu1" type="java.lang.Double">
 		<column name="U2RATEDU1" not-null="true" default="0" />  
 		</property> 
		
	</class>

</hibernate-mapping>
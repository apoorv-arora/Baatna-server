<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.application.baatna.bean.UserRating" table="UserRating">
		
		<id column="SERIAL" name="serial" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property name="rating" column="Rating" type="java.lang.Double" />
		<property name="reviewer" column="Reviewer" type="java.lang.Integer" />
		<property name="reviewed" column="Reviewed" type="java.lang.Integer" />

	</class>

</hibernate-mapping>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.application.baatna.bean.UserRating" table="USERRATING">
		<id column="SERIAL" name="serial" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property name="userIdOne" column="USERID_ONE" type="java.lang.Integer" />
		<property name="userIdTwo" column="USERID_TWO" type="java.lang.Integer" />
		<property name="rating" column="RATING" type="java.lang.Double" />
	</class>

</hibernate-mapping>
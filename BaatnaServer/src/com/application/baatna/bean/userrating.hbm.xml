<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.application.baatna.bean.UserRating" table="UserRating">
		
		<id column="SERIAL" name="serial" type="java.lang.Integer">
			<generator class="native" />
		</id>
		 <property name="rating" type="java.lang.Double">
 		<column name="Rating" not-null="true" default="0" />  
 		</property> 
		 <property name="reviewer" type="java.lang.Integer">
 		<column name="Reviewer" not-null="true" default="0" />  
 		</property> 
		 <property name="reviewed" type="java.lang.Integer">
 		<column name="Reviewed" not-null="true" default="0" />  
 		</property> 

	</class>

</hibernate-mapping>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.application.baatna.bean.Coupon" table="Coupon">

		<id column="Id" name="id" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property column="Validity" name="validity" type="java.lang.String" />
		<property column="Name" name="name" type="java.lang.String" />
		<property column="Terms" name="terms" type="java.lang.String" />
		<property name="count" type="java.lang.Integer">
			<column name="Count" not-null="true" default="0" />
		</property>
		<property column="Image" name="image" type="java.lang.String" />

	</class>
</hibernate-mapping>

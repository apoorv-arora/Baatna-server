<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.application.baatna.bean.Categories" table="CATEGORIES">

		<id column="CATEGORYID" name="categoryId" type="java.lang.Integer" />
		<property column="CATEGORY" name="category" type="java.lang.String" />
		<property column="CATEGORYICON" name="categoryIcon" type="java.lang.String" />
	</class>
</hibernate-mapping>

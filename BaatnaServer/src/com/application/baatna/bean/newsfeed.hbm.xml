<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.application.baatna.bean.NewsFeed" table="NEWSFEED">

		<id column="FEEDID" name="feedId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property name="type" type="java.lang.Integer">
 		<column name="TYPE" not-null="true" default="0" />  
 		</property>
		<property name="timestamp" type="java.lang.Long">
 		<column name="TIMESTAMP" not-null="true" default="0" />  
 		</property>
		<property name="userIdFirst" type="java.lang.Integer">
 		<column name="USER_ID_FIRST" not-null="true" default="0" />  
 		</property>
		<property name="userIdSecond" type="java.lang.Integer">
 		<column name="USER_ID_SECOND" not-null="true" default="0" />  
 		</property>
	    <property name="wishId" type="java.lang.Integer">
 		<column name="WISHID" not-null="true" default="0" />  
 		</property>
		
	</class>
</hibernate-mapping>
